   "source": [
    "# Plot distribution of genres (to detect any class imbalances)\n",
    "plt.figure(figsize = [8,8])\n",
    "pielabels = df.groupby('genre')['genre'].count().sort_values(ascending = False).index.tolist()\n",
    "plt.pie(df.groupby('genre')['genre'].count().sort_values(ascending = False), labels = pielabels, autopct='%1.1f%%')\n",
    "plt.title('Distribution of genres in song lyrics')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Downsample imbalanced classes"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "downsample_genre = ['Country','Hip-Hop','Metal','Pop','Rock']\n",
    "downsample_list = []\n",
    "\n",
    "for genre in df.genre.unique():\n",
    "    if genre in downsample_genre:\n",
    "        downsample_list.extend(np.random.choice(df[df.genre==genre].index,replace=False,size=10000))\n",
    "    else:\n",
    "        downsample_list.extend(df[df.genre==genre].index)\n",
    "        \n",
    "df_ds = df[df.index.isin(downsample_list)]\n",
    "df_ds.reset_index(inplace = True, drop=True)"
   ]
  },
